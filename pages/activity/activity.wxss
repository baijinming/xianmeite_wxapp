.container {
  padding-top: 66rpx;
  width: 750rpx;
  min-height: 100vh;
  background-color: #F6F6F6;
  box-sizing: border-box;
  overflow: hidden;
}

.navBar {
  width: 100%;
  background-color: #fff;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 996;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.nav-item {
  height: 66rpx;
  width: 120rpx;
  text-align: center;
  color: #222222;
  font-size: 12px;
  line-height: 66rpx;
}

.navBar .checked {
  border-bottom: 4rpx solid #F09B37;
}

.no-activity {
  margin: 250rpx auto 0;
  color: #9A9A9A;
  font-size: 12px;
  text-align: center
}

.no-activity .activity_icon {
  width: 164rpx;
  height: 164rpx;
  display: block;
  margin: 0 auto 24rpx;
}

.mask {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 123;
  background-color: #000;
  opacity: .5
}

.popup {
  width: 500rpx;
  position: fixed;
  left: 50%;
  top: 210rpx;
  z-index: 666;
  transform: translateX(-50%);
  background-color: #FFEEDA;
  border-radius: 10rpx;
}

.popup .fork-icon {
  position: absolute;
  right: 21rpx;
  top: 17rpx;
  width: 24rpx;
  height: 24rpx;
}

.popup .popup-title {
  color: #F09B37;
  font-size: 20px;
  margin-top: 50rpx;
  text-align: center
}

.popup .popup-img {
  width: 358rpx;
  height: 292rpx;
  display: block;
  margin: 0 auto;
}

.popup .lq-btn {
  width: 240rpx;
  height: 76rpx;
  background-color: #F09B37;
  color: #fff;
  line-height: 76rpx;
  font-size: 14px;
  text-align: center;
  margin: 20rpx auto 38rpx;
  border-radius: 38rpx;
}

.ac-header {
