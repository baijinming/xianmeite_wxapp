.container {
  border-top: 1rpx solid #DDDDDD;
  background-color: #F6F6F6;
  min-height: 100vh;
  width: 100%;
  box-sizing: border-box;
}

.proposal {
  background-color: #fff;
  width: 750rpx;
  height: 390rpx;
  color: #999999;
  font-size: 14px;
  line-height: 44rpx;
  padding: 40rpx 34rpx;
  box-sizing: border-box
}

.btn {
  width: 500rpx;
  height: 88rpx;
  background-color: #F09B37;
  border-radius: 44rpx;
  color: #fff;
  text-align: center;
  line-height: 88rpx;
  font-size: 16px;
  margin: 360rpx auto 0
}