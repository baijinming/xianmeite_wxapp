.container {
  background-color: #F6F6F6;
  min-height: 100vh;
  width: 100%;
  border-top: 1rpx solid #DDDDDD;
  box-sizing: border-box
}

.title {
  overflow: hidden;
  height: 284rpx;
  background-image: url('https://dstest.pxuc.com.cn/Public/imgs/integral_bg.png');
  background-size: 100% 100%;
  position: relative;
}

.cash-withdrawal {
  position: absolute;
  right: 18rpx;
  top: 20rpx;
  width: 125rpx;
  height: 45rpx;
  color: #C46B12;
  font-size: 24rpx;
  padding-top: 8rpx;
  box-sizing: border-box;
  text-align: center;
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,')
}

.integral-box {
  width: 186rpx;
  height: 186rpx;
  border:2rpx solid rgba(253,205,159,1);
  opacity:0.7;
  border-radius:50%;
  margin: 26rpx auto 18rpx;
  overflow: hidden;
  text-align: center
}

.integral-box .text1 {
  color: #F5F0F0;
  opacity: .8;
  font-size: 26rpx;
  margin-top: 38rpx;
}

.integral-box .now-integral {
  color: #fff;
  font-size: 60rpx;
}

.total-integral {
  margin-top: 18rpx;
  text-align: center;
  color: #FEFEFE;
  font-size: 12px;
}

.detaile-waper {
  margin-top: 20rpx;
  background-color: #fff;
  overflow: hidden;
}

.item {
  height: 150rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 37rpx 0 40rpx;
  border-bottom: 1px solid #F6F6F6
}

.item .operation {
  color: #000;
  font-size: 14px;
  margin-bottom: 20rpx;
}

.item .time {
  color: #999999;
  font-size: 12px;
}

.item .right {
  color: #222;
  font-size: 16px;
}