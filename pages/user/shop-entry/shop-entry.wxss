.container {
  background-color: #F6F6F6;
  width: 100%;
  min-height: 100vh
}

.form {
  border-top: 1rpx solid #F6F6F6;
  background-color: #fff;
  min-height: 100vh;
  box-sizing: border-box
}

.form-item {
  height: 110rpx;
  width: 100%;
  display: flex;
  align-items: center;
  color: #222222;
  font-size: 14px;
  border-bottom: 1rpx solid #F6F6F6;
  padding: 0 33rpx;
  box-sizing: border-box
}

.form-item .title {
  margin-right: 20rpx;
}

.form-item input {
  color:#222;
  font-size:14px;
  flex: 1
}

.form-item input::placeholder {
  color:#999999;
  font-size:14px
}

.form-item textarea {
  flex: 1;
  height: 60rpx;
  color: #222
}

.img-box {
  margin: 50rpx 60rpx 60rpx;
  display: flex;
  justify-content: space-between;
}

.img-bg {
  width: 300rpx;
  height: 300rpx;
  background-color: #EBEBEB;
  border-radius: 10rpx;
}

.img-bg .add-icon {
  width: 80rpx;
  height: 80rpx;
  margin: 80rpx auto 30rpx;
  display: block
}

.img-bg .bg-text {
  color: #999999;
  font-size: 14px;
  text-align: center;
}

.item-img {
  width: 300rpx;
  height: 300rpx;
  border-radius: 10rpx;
}

.license {
  width:600rpx;
  height:300rpx;
  background:rgba(235,235,235,1);
  border-radius:10rpx;
  margin: 80rpx auto 0;
  overflow: hidden;
}

.add-icon {
  width: 80rpx;
  height: 80rpx;
  margin: 110rpx auto 0;
  display: block;
}

.license-text {
  margin-top: 30rpx;
  text-align: center;
  color: #999999;
  font-size: 14px;
}

.license-img {
  width:600rpx;
  height:300rpx;
  margin: 80rpx auto 0;
  display: block
}

.btn {
  width:500rpx;
  height:88rpx;
  line-height: 88rpx;
  text-align: center;
  background:rgba(240,155,55,1);
  border-radius:44rpx;
  color: #fff;
  font-size: 16px;
}

.approved {
  overflow: hidden;
}

.success_icon {
  width: 164rpx;
  height: 164rpx;
  margin: 120rpx auto 30rpx;
  display: block
}

.approved-text {
  color: #9A9A9A;
  font-size: 12px;
  text-align: center;
  margin-bottom: 120rpx;
}

.prompt-box {
  background-color: #fff;
  padding: 80rpx 30rpx;
}

.prompt-text1 {
  color: #333333;
  font-size: 14px;
}

.prompt-text2 {
  color: #F09B37;
  font-size: 12px;
  margin: 20rpx 0 40rpx;
}

.prompt-textBox {
  width:690rpx;
  height:70rpx;
  background:rgba(237,237,237,1);
  border-radius:10rpx;
  display: flex;
  align-items: center;
  position: relative;
  margin-bottom: 30rpx;
}

.prompt-textBox-name {
  color: #666666;
  font-size: 14px;
  margin-left: 20rpx;
  width: 150rpx;
}

.prompt-textBox-content {
  color: #999999;
  font-size: 12px;
}

.edit-go {
  position: absolute;
  top: 50%;
  right: 25rpx;
  transform: translateY(-50%);
  text-decoration: underline;
  color: #F09B37;
  font-size: 12px;
}

.edit-password-box {
  overflow: hidden;
  padding: 50rpx 30rpx 0;
}

.editPass-textBox {
  height: 80rpx;
}

.editPass-textBox-name {
  color: #666666
}

.prompt-textBox-input {
  flex: 1;
  font-size: 12px;
}

.editPass-btn {
  width:588rpx;
  height:76rpx;
  background:rgba(240,155,55,1);
  border-radius:6rpx;
  color: #fff;
  text-align: center;
  line-height: 76rpx;
  font-size: 14px;
  margin-top: 100rpx;
}

.audit-failure {
  overflow: hidden
}

.fail-title {
  color: #333333;
  font-size: 14px;
  margin-bottom: 20rpx;
}

.fail-reason {
  color: #666666;
  font-size: 14px;
  margin-bottom: 20rpx;
}

.fillAgain-btn {
  width:588rpx;
  height:76rpx;
  background:rgba(240,155,55,1);
  border-radius:6rpx;
  color: #fff;
  font-size: 14px;
  line-height: 76rpx;
  text-align: center;
  margin-top: 80rpx;
}

.wait-examine {
  overflow: hidden;
}