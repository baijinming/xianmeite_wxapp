.container {
  border-top: 1px solid #DDDDDD;
  width: 100%;
}

.item {
  overflow: hidden
}

.header {
  display: flex;
  align-items: center;
  margin: 30rpx 0 0 33rpx;
}

.time {
  color: #999999;
  font-size: 14px;
  margin-right: 10rpx;
  font-weight: 400;
}

.line {
  height: 2rpx;
  background-color: #F6F6F6;
  flex: 1
}

.content {
  display: flex;
  flex-wrap: wrap;
  margin-left: 35rpx;
}

.content .item {
  width: 200rpx;
  margin: 40rpx 35rpx 0 0;
}

.content image {
  width: 200rpx;
  height: 200rpx;
  margin-bottom: 20rpx;
}

.content .title {
  font-size: 14px;
  color: #222222;
  text-align: center;
  white-space:nowrap; 
  overflow:hidden;
  text-overflow:ellipsis;
}